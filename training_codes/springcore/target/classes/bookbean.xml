<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd 
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context.xsd">
<bean id="book" class="com.ustglobal.springcore.di.Book" >
<property name="name" value="Reshu"/>
<property name="price" value="50000"/>
<property name="author" ref="author"/>
</bean>
<bean id="author" class="com.ustglobal.springcore.di.Author" init-method="init" destroy-method="destroy">
<property name="name" value="Kelin" />
<property name="penName" value="xyz"/>
</bean>
</beans>